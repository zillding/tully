{
  "name": "tully",
  "version": "0.0.0",
  "description": "An Electron application",
  "main": "main.js",
  "scripts": {
    "prettier": "prettier --write '**/*.{js,css,md}'",
    "start": "electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build"
  },
  "build": {
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ]
  },
  "repository": "github:zillding/tully",
  "keywords": [
    "electron"
  ],
  "author": "Zill Ding",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "next": "^8.0.1",
    "prettier": "^1.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-next": "^3.1.5"
  }
}
